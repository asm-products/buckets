  "devDependencies": {
    "bootstrap": "~3.2.0",
    "chaplin": "~1.0.0",
    "modernizr": "~2.8.1",
    "swag": "~0.7.0",
    "toastr": "~2.0.3",
    "ladda": "~0.9.3",
    "jquery.formParams": "~0.1.1",
    "underscore": "~1.6.0",
    "ace-builds": "~1.1.3",
    "cocktail": "~0.5.7",
    "fastclick": "~1.0.2",
    "gsap": "~1.13.1",
    "seedrandom": "~2.3.6",
    "Sortable": "~0.3.0"
  },
  "exportsOverride": {
    "jquery.formParams": {
      "js": "jquery.formParams.js"
